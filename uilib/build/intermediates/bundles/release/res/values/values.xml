<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="siArrowPosition">
        <enum name="left" value="0"/>
        <enum name="right" value="1"/>
    </attr>
    <attr name="siBorderType">
        <enum name="stroke" value="0"/>
        <enum name="fill" value="1"/>
    </attr>
    <attr name="siStrokeCap">
        <enum name="butt" value="0"/>
        <enum name="round" value="1"/>
        <enum name="square" value="2"/>
    </attr>
    <attr name="siStrokeJoin">
        <enum name="bevel" value="0"/>
        <enum name="miter" value="1"/>
        <enum name="round" value="2"/>
    </attr>
    <declare-styleable name="ClearableEditText"><attr format="dimension" name="ce_padding"/><attr format="reference" name="ce_drawable"/></declare-styleable>
    <declare-styleable name="PercentBar"><attr format="integer" name="percent_current"/><attr format="integer" name="percent_max"/><attr format="color" name="percent_unreached_color"/><attr format="color" name="percent_reached_color"/><attr format="dimension" name="percent_reached_bar_height"/><attr format="dimension" name="percent_unreached_bar_height"/><attr format="dimension" name="percent_text_size"/><attr format="color" name="percent_text_color"/><attr format="dimension" name="percent_text_offset"/><attr format="enum" name="percent_text_visibility"><enum name="visible" value="0"/><enum name="invisible" value="1"/></attr></declare-styleable>
